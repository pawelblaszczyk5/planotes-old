{
  "private": true,
  "name": "planotes",
  "description": "",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:css && npm run build:remix",
    "dev": "concurrently \"npm:dev:*\"",
    "test": "test-storybook",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx ./app",
    "format": "prettier --write \"./app/**/*.{js,jsx,ts,tsx,html,css,json}\"",
    "serve": "env-cmd remix-serve build",
    "dev:remix": "env-cmd -f .env.local remix dev",
    "dev:css": "postcss app/styles/index.css -o app/index.css --w",
    "dev:storybook": "start-storybook -p 6006",
    "build:css": "postcss app/styles/index.css -o app/index.css --env production",
    "build:remix": "remix build",
    "build:storybook": "build-storybook",
    "ci:lint": "eslint --ext .ts,.tsx,.js,.jsx ./app --max-warnings 0",
    "ci:tsc": "tsc",
    "ci:test": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run build:storybook --quiet && http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && npm test\"",
    "postinstall": "remix setup node && node scripts/postInstall.js",
    "render:build": "npm ci && npm run build && prisma migrate deploy",
    "render:start": "remix-serve build"
  },
  "dependencies": {
    "@fontsource/manrope": "^4.5.9",
    "@prisma/client": "^3.14.0",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "clsx": "^1.1.1",
    "framer-motion": "^6.3.5",
    "nodemailer": "^6.7.5",
    "prisma": "^3.14.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "remix": "^1.5.1",
    "remix-auth": "^3.2.2",
    "remix-auth-email-link": "^1.3.1",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@remix-run/dev": "^1.5.1",
    "@storybook/addon-a11y": "^6.5.6",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/react": "^6.5.6",
    "@storybook/test-runner": "^0.1.1",
    "@storybook/testing-library": "^0.0.11",
    "@tsconfig/strictest": "^1.0.1",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.2.1",
    "cssnano": "^5.1.10",
    "env-cmd": "^10.1.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.4.5",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-storybook": "^0.5.12",
    "eslint-plugin-testing-library": "^5.5.1",
    "http-server": "^14.1.0",
    "jest": "^27.5.1",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.7.2",
    "wait-on": "^6.0.1",
    "webpack": "^5.72.1"
  },
  "resolutions": {
    "jest": "^27.5.1",
    "jest-runner": "^27.5.1",
    "jest-environment-node": "27.5.1"
  },
  "engines": {
    "node": "=16"
  },
  "sideEffects": false,
  "volta": {
    "node": "16.14.0"
  }
